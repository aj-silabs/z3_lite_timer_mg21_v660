     44          #if defined(MBEDTLS_THREADING_PTHREAD)
     45          static void threading_mutex_init_pthread( mbedtls_threading_mutex_t *mutex )
     46          {
     47              if( mutex == NULL )
     48                  return;
     49          
     50              mutex->is_valid = pthread_mutex_init( &mutex->mutex, NULL ) == 0;
     51          }
     52          
     53          static void threading_mutex_free_pthread( mbedtls_threading_mutex_t *mutex )
     54          {
     55              if( mutex == NULL || !mutex->is_valid )
     56                  return;
     57          
     58              (void) pthread_mutex_destroy( &mutex->mutex );
     59              mutex->is_valid = 0;
     60          }
     61          
     62          static int threading_mutex_lock_pthread( mbedtls_threading_mutex_t *mutex )
     63          {
     64              if( mutex == NULL || ! mutex->is_valid )
     65                  return( MBEDTLS_ERR_THREADING_BAD_INPUT_DATA );
     66          
     67              if( pthread_mutex_lock( &mutex->mutex ) != 0 )
     68                  return( MBEDTLS_ERR_THREADING_MUTEX_ERROR );
     69          
     70              return( 0 );
     71          }
     72          
     73          static int threading_mutex_unlock_pthread( mbedtls_threading_mutex_t *mutex )
     74          {
     75              if( mutex == NULL || ! mutex->is_valid )
     76                  return( MBEDTLS_ERR_THREADING_BAD_INPUT_DATA );
     77          
     78              if( pthread_mutex_unlock( &mutex->mutex ) != 0 )
     79                  return( MBEDTLS_ERR_THREADING_MUTEX_ERROR );
     80          
     81              return( 0 );
     82          }
     83          
     84          void (*mbedtls_mutex_init)( mbedtls_threading_mutex_t * ) = threading_mutex_init_pthread;
     85          void (*mbedtls_mutex_free)( mbedtls_threading_mutex_t * ) = threading_mutex_free_pthread;
     86          int (*mbedtls_mutex_lock)( mbedtls_threading_mutex_t * ) = threading_mutex_lock_pthread;
     87          int (*mbedtls_mutex_unlock)( mbedtls_threading_mutex_t * ) = threading_mutex_unlock_pthread;
