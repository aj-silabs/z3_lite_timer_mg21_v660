     80          /*
     81           * ARC4 key schedule
     82           */
     83          void mbedtls_arc4_setup( mbedtls_arc4_context *ctx, const unsigned char *key,
     84                           unsigned int keylen )
     85          {
     86              int i, j, a;
     87              unsigned int k;
     88              unsigned char *m;
     89          
     90              ctx->x = 0;
     91              ctx->y = 0;
     92              m = ctx->m;
     93          
     94              for( i = 0; i < 256; i++ )
     95                  m[i] = (unsigned char) i;
     96          
     97              j = k = 0;
     98          
     99              for( i = 0; i < 256; i++, k++ )
    100              {
    101                  if( k >= keylen ) k = 0;
    102          
    103                  a = m[i];
    104                  j = ( j + a + key[k] ) & 0xFF;
    105                  m[i] = m[j];
    106                  m[j] = (unsigned char) a;
    107              }
    108          }
