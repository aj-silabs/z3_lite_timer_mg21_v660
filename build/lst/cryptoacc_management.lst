     26          #if defined( MBEDTLS_THREADING_C )
     27          static mbedtls_threading_mutex_t    cryptoacc_lock;
     28          static volatile bool                cryptoacc_lock_initialized = false;
     29          #endif /* MBEDTLS_THREADING_C */
     30          
     31          /* Get ownership of an available CRYPTOACC device */
     32          int cryptoacc_management_acquire( void )
     33          {
     34              int retVal;
     35          #if defined( MBEDTLS_THREADING_C )
     36              if ((SCB->ICSR & SCB_ICSR_VECTACTIVE_Msk) != 0U) {
     37                  return MBEDTLS_ERR_THREADING_MUTEX_ERROR;
     38              }
     39          
     40              /* Initialize mutex if that hasn't happened yet */
     41              CORE_DECLARE_IRQ_STATE;
     42          
     43              // Check flag first before going into a critical section, to avoid going into
     44              // a critical section on every single acquire() call. Since the _initialized
     45              // flag only transitions false -> true, we can in 99% of the calls avoid the
     46              // critical section.
     47              if ( !cryptoacc_lock_initialized ) {
     48                  CORE_ENTER_CRITICAL();
     49          
     50                  // Check the flag again after entering the critical section. Now that we're
     51                  // in the critical section, we can be sure that we are the only ones looking
     52                  // at the flag and no-one is interrupting us during its manipulation.
     53                  if ( !cryptoacc_lock_initialized ) {
     54                      mbedtls_mutex_init( &cryptoacc_lock );
     55                      cryptoacc_lock_initialized = true;
     56                  }
     57          
     58                  CORE_EXIT_CRITICAL( );
     59              }
     60          
     61              retVal = mbedtls_mutex_lock( &cryptoacc_lock );
     62          #else //!MBEDTLS_THREADING_C
     63              retVal = 0;
     64          #endif
     65              if (retVal == 0) {
     66                  CMU->CLKEN1 |= CMU_CLKEN1_CRYPTOACC;
     67                  CMU->CRYPTOACCCLKCTRL |= CMU_CRYPTOACCCLKCTRL_PKEN;
     68                  CMU->CRYPTOACCCLKCTRL |= CMU_CRYPTOACCCLKCTRL_AESEN;
     69              }
     70              return retVal;
     71          }
     72          
     73          /* Release ownership of an available CRYPTOACC device */
     74          int cryptoacc_management_release( void )
     75          {
     76              CMU->CLKEN1 = (CMU->CLKEN1 & ~CMU_CLKEN1_CRYPTOACC);
     77              CMU->CRYPTOACCCLKCTRL = (CMU->CRYPTOACCCLKCTRL & ~CMU_CRYPTOACCCLKCTRL_PKEN);
     78              CMU->CRYPTOACCCLKCTRL = (CMU->CRYPTOACCCLKCTRL & ~CMU_CRYPTOACCCLKCTRL_AESEN);
     79          #if defined( MBEDTLS_THREADING_C )
     80              return mbedtls_mutex_unlock(&cryptoacc_lock);
     81          #else //!MBEDTLS_THREADING_C
     82              return 0;
     83          #endif
     84          }
