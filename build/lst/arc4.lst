    110          /*
    111           * ARC4 cipher function
    112           */
    113          int mbedtls_arc4_crypt( mbedtls_arc4_context *ctx, size_t length, const unsigned char *input,
    114                          unsigned char *output )
    115          {
    116              int x, y, a, b;
    117              size_t i;
    118              unsigned char *m;
    119          
    120              x = ctx->x;
    121              y = ctx->y;
    122              m = ctx->m;
    123          
    124              for( i = 0; i < length; i++ )
    125              {
    126                  x = ( x + 1 ) & 0xFF; a = m[x];
    127                  y = ( y + a ) & 0xFF; b = m[y];
    128          
    129                  m[x] = (unsigned char) b;
    130                  m[y] = (unsigned char) a;
    131          
    132                  output[i] = (unsigned char)
    133                      ( input[i] ^ m[(unsigned char)( a + b )] );
    134              }
    135          
    136              ctx->x = x;
    137              ctx->y = y;
    138          
    139              return( 0 );
    140          }
