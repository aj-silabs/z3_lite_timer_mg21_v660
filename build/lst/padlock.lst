    129          /*
    130           * PadLock AES-CBC buffer en(de)cryption
    131           */
    132          int mbedtls_padlock_xcryptcbc( mbedtls_aes_context *ctx,
    133                                 int mode,
    134                                 size_t length,
    135                                 unsigned char iv[16],
    136                                 const unsigned char *input,
    137                                 unsigned char *output )
    138          {
    139              int ebx = 0;
    140              size_t count;
    141              uint32_t *rk;
    142              uint32_t *iw;
    143              uint32_t *ctrl;
    144              unsigned char buf[256];
    145          
    146              if( ( (long) input  & 15 ) != 0 ||
    147                  ( (long) output & 15 ) != 0 )
    148                  return( MBEDTLS_ERR_PADLOCK_DATA_MISALIGNED );
    149          
    150              rk = ctx->rk;
    151              iw = MBEDTLS_PADLOCK_ALIGN16( buf );
    152              memcpy( iw, iv, 16 );
    153          
    154               ctrl = iw + 4;
    155              *ctrl = 0x80 | ctx->nr | ( ( ctx->nr + ( mode ^ 1 ) - 10 ) << 9 );
    156          
    157              count = ( length + 15 ) >> 4;
    158          
    159              asm( "pushfl                        \n\t"
    160                   "popfl                         \n\t"
    161                   "movl    %%ebx, %0             \n\t"
    162                   "movl    %2, %%ecx             \n\t"
    163                   "movl    %3, %%edx             \n\t"
    164                   "movl    %4, %%ebx             \n\t"
