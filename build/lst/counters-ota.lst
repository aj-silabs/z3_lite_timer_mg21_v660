     27          static EmberStatus sendCluster(EmberNodeId destination,
     28                                         uint16_t clusterId,
     29                                         EmberMessageBuffer payload)
     30          {
     31            EmberApsFrame apsFrame;
     32            apsFrame.profileId = EMBER_PRIVATE_PROFILE_ID;
     33            apsFrame.clusterId = clusterId;
     34            apsFrame.sourceEndpoint = 0;
     35            apsFrame.destinationEndpoint = 0;
     36            apsFrame.options = (EMBER_APS_OPTION_RETRY
     37                                | EMBER_APS_OPTION_ENABLE_ROUTE_DISCOVERY);
     38            return emberSendUnicast(EMBER_OUTGOING_DIRECT,
     39                                    destination,
     40                                    &apsFrame,
     41                                    payload);
     42          }
     43          
     44          EmberStatus emberAfPluginCountersSendRequest(EmberNodeId destination,
     45                                                       bool clearCounters)
     46          {
     47            return sendCluster(destination,
     48                               (clearCounters
     49                                ? EMBER_REPORT_AND_CLEAR_COUNTERS_REQUEST
     50                                : EMBER_REPORT_COUNTERS_REQUEST),
     51                               EMBER_NULL_MESSAGE_BUFFER);
     52          }
     53          
     54          bool emberAfPluginCountersIsIncomingRequest(EmberApsFrame *apsFrame, EmberNodeId sender)
     55          {
     56            EmberMessageBuffer reply = EMBER_NULL_MESSAGE_BUFFER;
     57            uint8_t length = 0;
     58            uint8_t i;
     59            uint8_t temp[3];
     60          
     61            if (apsFrame->profileId != EMBER_PRIVATE_PROFILE_ID
     62                || (apsFrame->clusterId != EMBER_REPORT_COUNTERS_REQUEST
     63                    && apsFrame->clusterId != EMBER_REPORT_AND_CLEAR_COUNTERS_REQUEST)) {
     64              return false;
     65            }
     66          
     67            for (i = 0; i < EMBER_COUNTER_TYPE_COUNT; i++) {
     68              uint16_t val = emberCounters[i];
     69              if (val != 0) {
     70                temp[0] = i;
     71                temp[1] = LOW_BYTE(val);
     72                temp[2] = HIGH_BYTE(val);
     73                if (reply == EMBER_NULL_MESSAGE_BUFFER) {
     74                  reply = emberAllocateStackBuffer();
     75                  if (reply == EMBER_NULL_MESSAGE_BUFFER) {
     76                    return true;
     77                  }
     78                }
     79                if (emberAppendToLinkedBuffers(reply, temp, 3) != EMBER_SUCCESS) {
     80                  emberReleaseMessageBuffer(reply);
     81                  return true;
     82                }
     83                length += 3;
     84              }
     85              if (length + 3 > MAX_PAYLOAD_LENGTH
     86                  || (length
     87                      && (i + 1 == EMBER_COUNTER_TYPE_COUNT))) {
     88                // The response cluster is the request id with the high bit set.
     89                sendCluster(sender, apsFrame->clusterId | 0x8000, reply);
     90                emberReleaseMessageBuffer(reply);
     91                reply = EMBER_NULL_MESSAGE_BUFFER;
     92                length = 0;
     93              }
     94            }
     95          
     96            return true;
     97          }
     98          
     99          bool emberAfPluginCountersIsIncomingResponse(EmberApsFrame *apsFrame)
    100          {
    101            return (apsFrame->profileId == EMBER_PRIVATE_PROFILE_ID
    102                    && (apsFrame->clusterId == EMBER_REPORT_AND_CLEAR_COUNTERS_RESPONSE
    103                        || apsFrame->clusterId == EMBER_REPORT_COUNTERS_RESPONSE));
    104          }
    105          
    106          bool emberAfPluginCountersIsOutgoingResponse(EmberApsFrame *apsFrame,
    107                                                       EmberStatus status)
    108          {
    109            bool isResponse = emberIsIncomingCountersResponse(apsFrame);
    110            if (isResponse
    111                && apsFrame->clusterId == EMBER_REPORT_AND_CLEAR_COUNTERS_RESPONSE
    112                && status == EMBER_SUCCESS) {
    113              emberClearCounters();
    114            }
    115            return isResponse;
    116          }
