     58          /*
     59           * PadLock detection routine
     60           */
     61          int mbedtls_padlock_has_support( int feature )
     62          {
     63              static int flags = -1;
     64              int ebx = 0, edx = 0;
     65          
     66              if( flags == -1 )
     67              {
     68                  asm( "movl  %%ebx, %0           \n\t"
     69                       "movl  $0xC0000000, %%eax  \n\t"
     70                       "cpuid                     \n\t"
     71                       "cmpl  $0xC0000001, %%eax  \n\t"
     72                       "movl  $0, %%edx           \n\t"
     73                       "jb    unsupported         \n\t"
     74                       "movl  $0xC0000001, %%eax  \n\t"
     75                       "cpuid                     \n\t"
     76                       "unsupported:              \n\t"
     77                       "movl  %%edx, %1           \n\t"
     78                       "movl  %2, %%ebx           \n\t"
     79                       : "=m" (ebx), "=m" (edx)
     80                       :  "m" (ebx)
     81                       : "eax", "ecx", "edx" );
     82          
     83                  flags = edx;
     84              }
     85          
     86              return( flags & feature );
     87          }
