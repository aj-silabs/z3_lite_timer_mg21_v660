     89          /*
     90           * PadLock AES-ECB block en(de)cryption
     91           */
     92          int mbedtls_padlock_xcryptecb( mbedtls_aes_context *ctx,
     93                                 int mode,
     94                                 const unsigned char input[16],
     95                                 unsigned char output[16] )
     96          {
     97              int ebx = 0;
     98              uint32_t *rk;
     99              uint32_t *blk;
    100              uint32_t *ctrl;
    101              unsigned char buf[256];
    102          
    103              rk  = ctx->rk;
    104              blk = MBEDTLS_PADLOCK_ALIGN16( buf );
    105              memcpy( blk, input, 16 );
    106          
    107               ctrl = blk + 4;
    108              *ctrl = 0x80 | ctx->nr | ( ( ctx->nr + ( mode^1 ) - 10 ) << 9 );
    109          
    110              asm( "pushfl                        \n\t"
    111                   "popfl                         \n\t"
    112                   "movl    %%ebx, %0             \n\t"
    113                   "movl    $1, %%ecx             \n\t"
    114                   "movl    %2, %%edx             \n\t"
    115                   "movl    %3, %%ebx             \n\t"
    116                   "movl    %4, %%esi             \n\t"
    117                   "movl    %4, %%edi             \n\t"
    118                   ".byte  0xf3,0x0f,0xa7,0xc8    \n\t"
    119                   "movl    %1, %%ebx             \n\t"
    120                   : "=m" (ebx)
    121                   :  "m" (ebx), "m" (ctrl), "m" (rk), "m" (blk)
    122                   : "memory", "ecx", "edx", "esi", "edi" );
    123          
    124              memcpy( output, blk, 16 );
    125          
    126              return( 0 );
    127          }
