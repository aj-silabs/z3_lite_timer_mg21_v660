     37          /***************************************************************************//**
     38           * @brief
     39           *   Initialize the CRYOTIMER.
     40           *
     41           * @details
     42           *   Use this function to initialize the CRYOTIMER.
     43           *   Select a prescaler setting and select a low-frequency oscillator.
     44           *   See the configuration structure @ref CRYOTIMER_Init_TypeDef for more
     45           *   details.
     46           *
     47           * @param[in] init
     48           *   A pointer to the initialization structure.
     49           ******************************************************************************/
     50          void CRYOTIMER_Init(const CRYOTIMER_Init_TypeDef *init)
     51          {
     52            CRYOTIMER->PERIODSEL = (uint32_t)init->period & _CRYOTIMER_PERIODSEL_MASK;
     53            CRYOTIMER->CTRL = ((init->enable ? 1UL : 0UL) << _CRYOTIMER_CTRL_EN_SHIFT)
     54                              | ((init->debugRun ? 1UL : 0UL)
     55                                 << _CRYOTIMER_CTRL_DEBUGRUN_SHIFT)
     56                              | ((uint32_t)init->osc << _CRYOTIMER_CTRL_OSCSEL_SHIFT)
     57                              | ((uint32_t)init->presc << _CRYOTIMER_CTRL_PRESC_SHIFT);
     58            CRYOTIMER_EM4WakeupEnable(init->em4Wakeup);
     59          }
