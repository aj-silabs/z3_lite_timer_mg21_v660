     65          
     66                if ( ret == SE_RESPONSE_OK ) {
     67                  *olen = len;
     68                }
     69              }
     70          
     71              if ( surplus_bytes != 0 ) {
     72                SE_Command_t command2 = SE_COMMAND_DEFAULT(SE_COMMAND_TRNG_GET_RANDOM);
     73                SE_DataTransfer_t word_out = SE_DATATRANSFER_DEFAULT(&surplus_word, 4);
     74          
