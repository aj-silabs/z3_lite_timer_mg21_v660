     38          #define DO_TRNG_COND_TEST  (1)
     39          
     40          bool trng_initialized = false;
     41          
     42          int mbedtls_hardware_poll( void *data,
     43                                     unsigned char *output,
     44                                     size_t len,
     45                                     size_t *olen ) {
     46              int lock_status;
     47              uint32_t status;
     48              block_t data_out;
     49              size_t surplus_bytes = 0;
     50              uint32_t surplus_word = 0;
     51              (void) data;
     52          
     53              if(!trng_initialized) {
     54                  lock_status = cryptoacc_management_acquire();
     55                  if (lock_status != 0) {
     56                      return lock_status;
     57                  }
     58                  status = sx_trng_init(DO_TRNG_COND_TEST);
     59                  cryptoacc_management_release();
     60                  if (status != CRYPTOLIB_SUCCESS) {
     61                      return MBEDTLS_ERR_ENTROPY_SOURCE_FAILED;
     62                  }
     63                  trng_initialized = true;
     64              }
     65          
     66              // Store output length since we can expect sx_rng_get_rand_blk to
     67              // return exactly the requested number of words (return type is void).
     68              *olen = len;
     69          
     70              // The sx_rng_get_rand_blk function returns full 32-bit words
     71              // of random data. If the requested number of bytes (and output buffer)
     72              // is not a multiple of 4 bytes we need to get the remaining/surplus
     73              // bytes in an additional call.
     74              surplus_bytes = len & 0x3;
     75              len &= ~0x3;
     76          
     77              if ( len > 0 ) {
     78                  // Setup data block and get random data.
     79                  data_out = block_t_convert(output, len);
     80                  lock_status = cryptoacc_management_acquire();
     81                  if (lock_status != 0) {
     82                      return lock_status;
     83                  }
     84                  sx_trng_get_rand_blk(data_out);
     85                  cryptoacc_management_release();
     86              }
     87          
     88              if ( surplus_bytes != 0 ) {
     89                  // Setup data block and get random data.
     90                  data_out = block_t_convert(&surplus_word, 4);
     91                  lock_status = cryptoacc_management_acquire();
     92                  if (lock_status != 0) {
     93                      return lock_status;
     94                  }
     95                  sx_trng_get_rand_blk(data_out);
     96                  cryptoacc_management_release();
     97                  for ( size_t i = 0; i < surplus_bytes; i++ ) {
     98                    output[len+i] = (surplus_word >> (i*8)) & 0xFF;
     99                  }
    100              }
    101          
    102              return 0;
    103          }
